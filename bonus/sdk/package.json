{
  "name": "epytodo-sdk",
  "version": "1.1.0",
  "description": "OpenAPI generated SDK for Epytodo backend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/index.js.map"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "dts": "dts-bundle-generator --project tsconfig.json --out-file dist/index.d.ts src/index.ts",
    "build": "rspack build",
    "generate": "openapi --input http://localhost:3000/openapi-specs.yaml --output ./src --client fetch && npm run generate:post",
    "generate:post": "npm run lint && npm run format",
    "prepublishOnly": "npm run clean && npm run dts && npm run build",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "api",
    "sdk",
    "openapi",
    "typescript"
  ],
  "author": "Ely Delva @ Dimitri Collineau",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebackslashs/epytodo.git"
  },
  "bugs": {
    "url": "https://github.com/thebackslashs/epytodo/issues"
  },
  "homepage": "https://github.com/thebackslashs/epytodo#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rspack/cli": "^1.3.10",
    "@rspack/core": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.2.5",
    "typescript": "^5.8.3"
  }
}
