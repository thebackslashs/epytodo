{
  "name": "epytodo",
  "version": "1.0.0",
  "description": "Epitech Web Todo List Project",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest ./src",
    "test:e2e": "jest --config e2e/jest.config.js",
    "build": "rspack build",
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write 'src/**/*.ts' --check",
    "format:fix": "prettier --write 'src/**/*.ts'",
    "migrate": "tsx migrate.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebackslashs/epytodo.git"
  },
  "keywords": [
    "API",
    "ExpressJS",
    "Epitech",
    "Web",
    "Project"
  ],
  "author": "Ely Delva @ Dimitri Collineau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebackslashs/epytodo/issues"
  },
  "homepage": "https://github.com/thebackslashs/epytodo#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "ts-jest": "^29.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@rspack/cli": "^1.3.10",
    "@rspack/core": "^1.3.10",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
